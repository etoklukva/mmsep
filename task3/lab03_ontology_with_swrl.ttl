[ swrla:isRuleEnabled "true"^^xsd:boolean
] .

#################################################################
#    Rules
#################################################################

:Q rdf:type swrl:Variable .

:S rdf:type swrl:Variable .

:P rdf:type swrl:Variable .

:R rdf:type swrl:Variable .

[ rdfs:label "Rule: classify valid query" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :Запрос ;
                           swrl:argument1 :Q
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate :автор_запроса ;
                                      swrl:argument1 :Q ;
                                      swrl:argument2 :S
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate :обучается_по ;
                                                 swrl:argument1 :S ;
                                                 swrl:argument2 :P
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                            swrl:propertyPredicate :содержит ;
                                                            swrl:argument1 :P ;
                                                            swrl:argument2 :R
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                       swrl:propertyPredicate :ссылается_на ;
                                                                       swrl:argument1 :Q ;
                                                                       swrl:argument2 :R
                                                                     ] ;
                                                           rdf:rest [ rdf:type swrl:AtomList ;
                                                                      rdf:first [ rdf:type swrl:ClassAtom ;
                                                                                  swrl:classPredicate :УчебныйМатериал ;
                                                                                  swrl:argument1 :R
                                                                                ] ;
                                                                      rdf:rest rdf:nil
                                                                    ]
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :КорректныйЗапрос ;
                           swrl:argument1 :Q
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

